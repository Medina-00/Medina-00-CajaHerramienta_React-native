{
  "expo": {
    "name": "Tarea6",
    "slug": "Tarea6",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fotoPerfil.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fotoPerfil.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hola_mundo_12.Tarea6"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b8e3c13f-8852-423f-9687-14461d3dd6f3"
      }
    }
  }
}
